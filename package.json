{
  "name": "sf_abs_management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/sqlite": "^2.1.2",
    "@sap-cloud-sdk/connectivity": "^4.2.0",
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap-cloud-sdk/resilience": "^4.2.0",
    "@sap/cds": "^9.5.2",
    "@sap/cds-dk": "^9.5.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.7",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.8",
    "@sapui5/ts-types": "^1.92.2",
    "compression": "^1.8.1",
    "eslint": "^7.32.0",
    "express": "^4",
    "in": "^0.19.0",
    "npm": "^11.7.0",
    "project": "^0.1.6"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "ci:cds-build": "cds build --production",  
    "start": "cds-serve",
    "watch-sfabshrsmgmt": "cds watch --open com.fluidra.rc.sfabshrsmgmt/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-sfabsmanagement": "cds watch --open com.fluidra.rc.sfabsmanagement/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-sfabsmgmt": "cds watch --open rc.com.fluidra.sfabsmgmt/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "auth": {
        "kind": "basic"
      },
      "db": {
        "kind": "sqlite",
        "model": "db/schema-model.cds",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/sfabshrsmgmt",
    "app/sfabsmanagement",
    "app/sfabsmgmt"
  ]
}
