{
  "name": "sf_abs_management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/sqlite": "^2.1.2",
    "@sap-cloud-sdk/connectivity": "^4.2.0",
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap-cloud-sdk/resilience": "^4.2.0",
    "@sap/cds": "^9.6.1",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.7",
    "compression": "^1.8.1",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/cds-dk": "^7",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.8",
    "@sapui5/ts-types": "^1.92.2",
    "cds-plugin-ui5": "^0.13.0",
    "eslint": "^7.32.0"
  },
  "scripts": {
    "start": "cds run --production"
  },
  "cds": {
    "requires": {
      "auth": {
        "kind": "xsuaa"
      },
      "db": {
        "kind": "sqlite",
        "model": "db/schema-model.cds",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/sfabshrsmgmt",
    "app/sfabsmanagement",
    "app/sfabsmgmt"
  ]
}
